# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General > Your apps
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Razorpay Configuration (for Firebase Functions)
# These are set using: firebase functions:config:set
# razorpay.key_id=rzp_test_your_key_id_here
# razorpay.key_secret=your_razorpay_key_secret_here

# Email Configuration (for Firebase Functions)
# These are set using: firebase functions:config:set
# email.user=your_email@gmail.com
# email.pass=your_app_password_here

# Environment
NODE_ENV=development
